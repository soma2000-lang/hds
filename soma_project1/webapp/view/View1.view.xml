<mvc:View controllerName="somaproject1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">

      <Table id="EmployeeID"
		items="{
			path: '/Empjob'
		}">
		<columns>
			<Column
				width="12em">
				<Text text="EmployeeInformation" />
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="EmployeeName" />
			</Column>
		</columns>
		<items>
			<ColumnListItem vAlign="Middle" press="onItemPress" type="Navigation">
				<cells>
				<ObjectIdentifier
						title="{EmployeeID}"/>
					<Text
						text="{Employee Name}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
    </Page>
</mvc:View>